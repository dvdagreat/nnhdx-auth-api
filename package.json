{
    "name": "auth.nnhdx.com",
    "version": "1.0.0",
    "description": "authentication api for nnhdx services",
    "main": "server.ts",
    "scripts": {
        "dev": "ts-node -r tsconfig-paths/register src/server.ts",
        "build": "rm -rf dist && tsc",
        "prod": "node -r ts-node/register -r tsconfig-paths/register dist/server.js",
        "test": "jest src/tests/",
        "test-watch": "jest --watch src/tests/",
        "test-coverage": "jest --coverage src/tests/"
    },
    "keywords": [
        "auth-api",
        "authentication"
    ],
    "author": "divyesh ladva <ladoodivyesh@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "joi": "^17.6.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.2.2"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.1",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/node": "^17.0.18",
        "@types/supertest": "^2.0.11",
        "jest": "^27.5.1",
        "nodemon": "^2.0.15",
        "supertest": "^6.2.2",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.5.0",
        "tsconfig-paths": "^3.12.0",
        "typescript": "^4.5.5",
        "typescript-paths": "^1.3.1"
    }
}
